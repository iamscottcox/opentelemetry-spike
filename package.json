{
  "name": "opentelemetry-spike",
  "version": "1.0.0",
  "description": "",
  "main": "app.ts",
  "scripts": {
    "start:prometheus": "docker run --rm -v ${PWD}/prometheus.yml:/prometheus/prometheus.yml -p 9090:9090 prom/prometheus --enable-feature=otlp-write-receive",
    "start:zipkin": "docker run --rm -p 9411:9411 --name zipkin openzipkin/zipkin",
    "start:collector": "docker run -p 4317:4317 -p 4318:4318 --rm -v $(pwd)/collector-config.yaml:/etc/otelcol/config.yaml otel/opentelemetry-collector",
    "start": "npx nodemon --require ./instrumentation.ts app.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@opentelemetry/api": "1.7.0",
    "@opentelemetry/auto-instrumentations-node": "0.40.0",
    "@opentelemetry/exporter-metrics-otlp-proto": "0.45.1",
    "@opentelemetry/exporter-prometheus": "0.45.1",
    "@opentelemetry/exporter-trace-otlp-proto": "0.45.1",
    "@opentelemetry/exporter-zipkin": "1.18.1",
    "@opentelemetry/resources": "1.18.1",
    "@opentelemetry/sdk-metrics": "1.18.1",
    "@opentelemetry/sdk-node": "0.45.1",
    "@opentelemetry/semantic-conventions": "1.18.1",
    "@types/express": "4.17.21",
    "@types/node": "20.9.0",
    "express": "4.18.2",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "nodemon": "3.0.1"
  }
}
